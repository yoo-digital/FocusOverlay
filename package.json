{
  "name": "@yoo-digital/focus-overlay",
  "version": "1.0.0",
  "description": "Library for creating overlays on focused elements.",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoo-digital/FocusOverlay.git"
  },
  "bugs": {
    "url": "https://github.com/yoo-digital/FocusOverlay/issues"
  },
  "homepage": "https://github.com/yoo-digital/FocusOverlay",
  "author": "YOO AG",
  "main": "dist/focusoverlay.cjs.js",
  "module": "dist/focusoverlay.esm.js",
  "unpkg": "dist/focusoverlay.js",
  "scripts": {
    "build": "rimraf dist && npm run build:es && npm run build:types",
    "build:es": "rollup -c rollup.config.js",
    "build:types": "tsc -p ./tsconfig.types.json --emitDeclarationOnly --outDir dist/types",
    "watch": "rollup -c -w",
    "lint": "eslint src/**/*.js",
    "lint:fix": "prettier-eslint src/**/*.js --write",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build && git add * && git commit -m \"Updating build\"",
    "release": "np patch --any-branch"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "focus",
    "overlay",
    "javascript",
    "library",
    "es6",
    "accessibility",
    "CSS",
    "a11y",
    "focusoverlay"
  ],
  "sideEffects": false,
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@yoo-digital/eslint-config-react": "^0.4.0",
    "@yoo-digital/stylelint-config-sass": "^0.3.1",
    "autoprefixer": "^10.1.0",
    "babel-plugin-array-includes": "^2.0.3",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^2.29.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "np": "^4.0.2",
    "postcss": "^8.2.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.3.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "^1.30.0",
    "stylelint": "^13.8.0",
    "stylelint-scss": "^3.18.0",
    "tslib": "^2.0.3",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
